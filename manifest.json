{
"manifest_version": 2,
    "name": "Depressed of Competitive Programming",
    "description": "A simple extension for those who wants to go far away for a limited time",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://www.facebook.com/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    }
}